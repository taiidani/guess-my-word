{
  "name": "web",
  "description": "A Vue.js project",
  "author": "Ryan Nixon <r.taiidani@gmail.com>",
  "private": true,
  "scripts": {
    "watchify": "watchify -vd -p browserify-hmr -e src/main.js -o dist/build.js",
    "serve": "http-server -o -s -c 1 -a localhost --proxy http://127.0.0.1:3000 --proxy-options.ws=true",
    "dev": "npm-run-all --parallel watchify serve",
    "build": "cross-env NODE_ENV=production browserify -g envify src/main.js | uglifyjs -c warnings=false -m > dist/build.js"
  },
  "dependencies": {
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-preset-env": "^1.7.0",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "browserify-hmr": "^0.4.1",
    "cross-env": "^7.0.3",
    "envify": "^4.1.0",
    "http-server": "^14.0.0",
    "npm-run-all": "^4.1.5",
    "uglify-js": "^2.5.0",
    "vueify": "^9.1.0",
    "watchify": "^4.0.0"
  },
  "browserify": {
    "transform": [
      "vueify",
      "babelify"
    ]
  },
  "browser": {
    "vue": "vue/dist/vue.common.js"
  }
}
